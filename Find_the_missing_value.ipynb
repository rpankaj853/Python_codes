    "def finder(arr,arr1):\n",
    "    arr.sort();\n",
    "    arr1.sort();\n",
    "    \n",
    "    for num, num2 in zip(arr,arr1):\n",
    "        if num != num2:\n",
    "            return num2\n",
    "        \n",
    "    return arr[-1]\n",
    "        \n",
    "        \n",
    "finder([1,2,3,4,6,7],[3,2,1,5,4,6,7])\n",
